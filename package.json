    "daily-maintenance": "ts-node src/scripts/daily-maintenance.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "restaurant",
    "admin",
    "api",
    "express",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
