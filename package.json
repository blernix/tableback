{
  "name": "tablemaster-api",
  "version": "1.0.0",
  "description": "TableMaster Backend API - Restaurant Administration Platform",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "seed:admin": "ts-node src/scripts/seed-admin.ts",
    "cleanup-indexes": "ts-node src/scripts/cleanup-redundant-indexes.ts",
    "cleanup:inactive": "ts-node src/scripts/cleanup-inactive-restaurants.ts",
